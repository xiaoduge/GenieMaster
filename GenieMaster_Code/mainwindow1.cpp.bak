    sleep(50);
    writeHex(m_HexCmd.hexOnX23);
    sleep(50);
    writeHex(m_HexCmd.hexWriteD50);
    sleep(50);
    writeHex(m_HexCmd.hexReadYout);
    sleep(50);
}

